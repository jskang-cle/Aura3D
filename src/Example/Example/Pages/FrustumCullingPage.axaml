<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:local="using:Aura3D.Avalonia"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Example.Pages.FrustumCullingPage">
  <DockPanel>
	  <StackPanel Orientation="Horizontal" DockPanel.Dock="Top">
		  <CheckBox x:Name="checkbox" Content="Frustum Culling" IsCheckedChanged="CheckBox_IsCheckedChanged"></CheckBox>
      </StackPanel>
      <local:Aura3DView x:Name="aura3Dview" SceneInitialized="Aura3DView_SceneInitialized" SceneUpdated="aura3Dview_SceneUpdated"/>
  </DockPanel>
</UserControl>
