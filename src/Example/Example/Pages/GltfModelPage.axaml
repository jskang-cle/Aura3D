<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             xmlns:local="clr-namespace:Aura3D.Avalonia;assembly=Aura3D.Avalonia"
             
             x:Class="Example.Pages.GltfModelPage">
  <DockPanel>
    <DockPanel DockPanel.Dock="Top" HorizontalAlignment="Left">
		<Button Margin="10"  Content="Open File" Click="Button_Click"></Button>
        <Button x:Name="lionButton" Margin="10"  Content="lion head" Click="Button_Click_1"></Button>
        <Button x:Name="soldierButton" Margin="10"  Content="soldier" Click="Button_Click_1"></Button>
        <Button x:Name="woodenButton" Margin="10"  Content="wooden stool" Click="Button_Click_1"></Button>
    </DockPanel>

     <local:Aura3DView x:Name="aura3d" SceneInitialized="Aura3DView_SceneInitialized" SceneUpdated="Aura3DView_SceneUpdated"></local:Aura3DView>
  </DockPanel>
</UserControl>
